ponse = await ProductAPI.putUpdateProduct(data);\r\n      console.log(response.message);\r\n      setFormData({\r\n        Name: \"\",\r\n        Category: \"\",\r\n        Price: 0,\r\n        ShortDes: \"\",\r\n        LongDes: \"\",\r\n        Images: [],\r\n        Quantity: 0,\r\n      });\r\n      document.getElementById(\"UpdateProduct\").reset();\r\n      window.alert(\"Cập nhật sản phẩm thành công\");\r\n      navigate(\"/products\");\r\n    } catch (error) {\r\n      console.error(\"Error uploading product\", error);\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div className=\"page-wrapper\">\r\n      <div className={styles.page_NewProduct}>\r\n        <div className={styles.tytile}>\r\n          <h1>Update Product</h1>\r\n        </div>\r\n        <div className=\"row\">\r\n          <form style={{ width: \"50%\", marginLeft: \"10%\" }} id=\"UpdateProduct\">\r\n            <div className=\"form-group\">\r\n              <label>Product Name</label>\r\n              <input\r\n                name=\"Name\"\r\n                onChange={handleInputChange}\r\n                value={formData.Name}\r\n                type=\"text\"\r\n                className=\"form-control\"\r\n                placeholder=\"Enter Product Name\"\r\n              />\r\n              {isErrors.Name && (\r\n                <span style={{ color: \"red\" }}>{errors.Name}</span>\r\n              )}\r\n            </div>\r\n            <div className=\"form-group\">\r\n              <label>Category</label>\r\n              <input\r\n                name=\"Category\"\r\n                onChange={handleInputChange}\r\n                value={formData.Category}\r\n                type=\"text\"\r\n                className=\"form-control\"\r\n                placeholder=\"Enter Category\"\r\n              />\r\n              {isErrors.Category && (\r\n                <span style={{ color: \"red\" }}>{errors.Category}</span>\r\n              )}\r\n            </div>\r\n            <div classNam